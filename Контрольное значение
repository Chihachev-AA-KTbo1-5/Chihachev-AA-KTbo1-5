#include <iostream>
#include <math.h>

using namespace std;
int main()
{

int N;
cin>>N;
int*B=new int [N];
for (int i = 0; i < N; i++)
		{
			cin>>B[i];
		}
int c=0;

for (int i = 0; i < N; i++)
		{
			c=c+B[i];
		}

if (c>0){
  cout<<c<<" POSITIVE";
}
if (c<0){
  cout<<c<<" NEGATIVE";
}
if (c==0){
  cout<<c<<" ZERO";
}




  return 0;
}
