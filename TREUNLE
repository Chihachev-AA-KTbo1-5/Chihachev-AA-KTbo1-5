#include<stdio.h>
#include <math.h>
int main()
{

	int a, b, c;
	printf("Vvedite storoni treunle");
	scanf_s("%d %d %d", &a, &b, &c);
	if (a + b > c || a + c > b || b + c > a)
	{
		printf("treungle is be");
		if (a == b || a == c || b == c)
		{
			printf("treungle ravnibedriny");
		}
		if (a == b && a == c & b == c)
		{
			printf("treungle ravnostoronniy");

		}
		if (a != b && a != c && b != c)
		{
			printf("trungle raznostoronniy");

		}





	}
	else
	{
		printf("treungle is not be");
	}





	return 0;
}
