#include <stdio.h>
#include <cstdlib>
#define F10
int main()
{
	
	int A[10];
	int i;
	for (i = 0; i < 10; i++)
	{
		A[i]=rand() % 100;
		printf("%d ", A[i]);
	

	}
	printf("\n");
	int B[10];
	for (i = 0; i < 10; i++)
	{
		B[i] = A[9 - i];
		printf("%d ", B[i]);
		
	}
	printf("\n");
	int C[10];
	for(i = 0; i < 10; i++)
	{
		C[i] = A[i] - B[i];
		printf("%d ", C[i]);
		
	}
	printf("\n");



		return 0;

}
