#include <stdio.h>
int main()
{

	int n;
	int a=0;
	int b=0;
	int c=0;
	
		do {
			scanf_s("%d", &n); //ввести число

		} while (n < 0 || n > 5); //будеть выполнятся пока n меньше 0 или больше 5

		
	
	 
		while (c<n)
		{
			scanf_s("%d", &a);
			b = b + a;
			c++;

		}

	
	printf ("%d", b);
	
	
	
	
	return 0;
}
